{
  "name": "gk-flutter-getx-pattern",
  "displayName": "GK Flutter GetX Pattern",
  "publisher": "GnyKoua",
  "description": "GK custom flutter design pattern using GetX",
  "repository": {
    "type": "public",
    "url": "git@github.com:GnyKoua/GK-FLUTTER-GETX-VSCODE-EXTENSION.git"
  },
  "icon": "./images/avatar.png",
  "badges": [],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
        "command": "gk-flutter-getx-pattern.getxinit",
        "title": "GK flutter GetX init"
      },
      {
        "command": "gk-flutter-getx-pattern.statefullfile",
        "title": "GK flutter statefull file"
      },
      {
        "command": "gk-flutter-getx-pattern.statelessfile",
        "title": "GK flutter stateless file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}